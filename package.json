{
  "name": "cdp-user-service-backend",
  "version": "0.0.0",
  "description": "CDP Backend Template",
  "main": "src/index.js",
  "type": "module",
  "engines": {
    "node": ">=22.13.1"
  },
  "scripts": {
    "dev": "NODE_ENV=development npm run server:watch",
    "dev:debug": "NODE_ENV=development npm run server:debug",
    "docker:dev": "npm run dev",
    "format": "prettier --write \"src/**/*.js\" \"**/*.{js,cjs,ts,md,json,config.js,test.js}\"",
    "format:check": "prettier --check \"src/**/*.js\" \"**/*.{js,cjs,ts,md,json,config.js,test.js}\"",
    "postinstall": "npm run setup:husky",
    "lint": "npm run lint:js",
    "lint:fix": "eslint . --fix",
    "lint:js": "eslint .",
    "postversion": "git add package.json package-lock.json && git commit -m $npm_package_version",
    "pretest": " node test-helpers/download-mongo.js",
    "security-audit": "npm audit --audit-level=high",
    "test": "TZ=UTC vitest run --coverage",
    "test:watch": "TZ=UTC vitest",
    "server:watch": "nodemon --exec tsx --enable-source-maps ./src",
    "server:debug": "nodemon --exec tsx --enable-source-maps --inspect=9249 ./src",
    "prestart": "npm run build",
    "start": "NODE_ENV=production node --use-strict .",
    "setup:husky": "node -e \"try { (await import('husky')).default() } catch (e) { if (e.code !== 'ERR_MODULE_NOT_FOUND') throw e }\" --input-type module"
  },
  "author": "Defra DDTS",
  "license": "OGL-UK-3.0",
  "dependencies": {
    "@aws-sdk/client-cognito-identity": "3.901.0",
    "@azure/identity": "4.12.0",
    "@date-fns/utc": "2.1.1",
    "@defra/cdp-validation-kit": "0.56.0",
    "@defra/hapi-tracing": "1.28.0",
    "@elastic/ecs-pino-format": "1.5.0",
    "@hapi/boom": "10.0.1",
    "@hapi/hapi": "21.4.3",
    "@hapi/inert": "7.1.0",
    "@hapi/jwt": "3.2.1",
    "@hapi/vision": "7.0.3",
    "@hapi/wreck": "18.1.0",
    "@microsoft/microsoft-graph-client": "3.0.7",
    "@octokit/auth-app": "8.1.1",
    "@octokit/core": "7.0.5",
    "@octokit/plugin-paginate-graphql": "6.0.0",
    "aws4": "1.13.2",
    "babel-plugin-module-resolver": "5.0.2",
    "convict": "6.2.4",
    "date-fns": "4.1.0",
    "global-agent": "3.0.0",
    "hapi-pino": "12.1.0",
    "hapi-pulse": "3.0.1",
    "https-proxy-agent": "7.0.6",
    "joi": "18.0.1",
    "joi-objectid": "4.0.2",
    "lodash": "4.17.21",
    "mongo-locks": "3.0.2",
    "mongodb": "6.20.0",
    "pino": "9.9.5",
    "tsx": "4.20.6",
    "undici": "7.16.0"
  },
  "devDependencies": {
    "@babel/cli": "7.28.3",
    "@babel/core": "7.28.5",
    "@babel/preset-env": "7.28.5",
    "@types/convict": "6.1.6",
    "@types/eslint": "9.6.1",
    "@types/lodash": "4.17.20",
    "@typescript-eslint/eslint-plugin": "8.45.0",
    "@typescript-eslint/parser": "8.45.0",
    "@vitest/coverage-v8": "3.2.4",
    "eslint": "9.35.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-jsdoc": "54.7.0",
    "eslint-plugin-n": "17.22.0",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-promise": "7.2.1",
    "eslint-plugin-vitest": "0.5.4",
    "globals": "16.4.0",
    "husky": "9.1.7",
    "mongodb-memory-server-core": "10.2.2",
    "neostandard": "0.12.2",
    "nodemon": "3.1.10",
    "npm-run-all": "4.1.5",
    "pino-pretty": "13.1.1",
    "prettier": "3.6.2",
    "vitest": "3.2.4",
    "vitest-fetch-mock": "0.4.5",
    "vitest-mongodb": "1.0.2"
  }
}
